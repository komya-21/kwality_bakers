<section id="main-content" class="animated fadeInUp">
                <div class="row">
                    <div class="col-md-12">
                        <div class=" panel panel-info">
                            <div class="panel-heading">
                                <h3 class="panel-title">Report</h3>
                                <div class="actions pull-right">
                                 <i class="fa fa-expand"></i>
                                    <i class="fa fa-chevron-down"></i>
                                    <i class="fa fa-times"></i>
                                </div>
                            </div>
                            <div class="panel-body">
<%= form_for :report, url:{action: 'create'} do |f| %>      

     <div class="form-group">
        <div class="col-md-4">
          <label class="col-sm-8 control-label">Select Start Date</label>
          <%= f.date_field :start_date,class: "form-control datepicker" %>
    </div>
  </div>

   <div class="form-group">
        <div class="col-md-4">
          <label class="col-sm-8 control-label">Select End Date</label>
          <%= f.date_field :end_date,class: "form-control datepicker" %>
    </div>
  </div>

    <div class="form-group">
        <div class="col-md-4">
          <label class="col-sm-6 control-label">Report Type</label>
          <%= f.select :report_type, options_for_select(['Payment Status', 'Return', 'Inward', 'Delivery Items','All Vendors(Custom/Regular)','Custom Deliveries','Regular Deliveries']),{prompt: true},class: "form-control" %>
    </div>
  </div>

<br>
<br>
<br>
<br>

  <div id="stage">
</div>

    <div class="form-group"> 
     <div class="col-sm-offset-1 col-sm-10">  
      <%= f.submit "View", class: 'btn btn-success' %>
     </div>
    </div>    

<% end %>

</div>
</div>
</div>
</div>
</section>
