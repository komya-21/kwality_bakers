<div class="maindiv">

<style>
table, th, td {
    border: 1px solid black;
    border-collapse: collapse;
}
</style>
</head>
<body>
<% k = 0 %>
<% @workorder.fproducts.each do |f| %>
<% k += 1 %>

<table style="width:100%">
  

  <% @letters = ('A'..'Z').to_a %>

  <% @uniq_ftypes = f.measurements.all.map{|m| m.ftype}.uniq %>
  <% @m = 0 %>
  <% @uniq_ftypes.each do |u| %>
  <% @m += 1 %>
  <tr>
    <th style="font-size: 16px; background-color: #ccc; width: 75px;" colspan="14"><center><% if @m == 1 %><%= f.product %><br><% end %>(<%= @letters.shift %>)<%= u %></center></th>
  </tr>
  <% @i = 0 %>
   <% if u == "Carcass Box" %>
   <tr>
    <th>No</th>
    <th>W</th>
    <th>D</th>
    <th>H</th>
    <th>Skirting</th>
    <th>Vertical</th>
    <th>Horizontal</th>
    <th>Color Side</th>
    <th>Nos</th>
    <th>Sq.ft 18mm</th>
    
    <th>Sq.ft8mm(back)</th>
    
    
  </tr>
   <% @carcass_measurements = f.measurements.where(ftype: u) %>
    <% @total_no = 0 %>
    <% @total_sqft = 0 %>
    <% @total_sqft_back = 0 %>
    <% @total_amount = 0 %>
    <%# @carcass_rate1 = @rates.find_by(["product = ? and ptype=? and ctype = ?",f.product,u,"TB/LR"]) %>
<%# @carcass_rate2 = @rates.find_by(["product = ? and ptype=? and ctype = ?",f.product,u,"Back"]) %>
<%# @c1 = @carcass_rate1.price %>
<%# @c2 = @carcass_rate2.price %>
   <% @carcass_measurements.each do |c| %>
   <% @i += 1 %>
   <% @total_no += c.quantity %>
   <tr>
   <td><%= @i %></td>
   <td><%= c.width %></td>
   <td><%= c.depth %></td>
   <td><%= c.height %></td>
   <td><%= c.skirting %></td>
   <td><%= c.vertical%></td>
   <td><%= c.horizontal %></td>
   <td>
   <% if c.right == "Yes" %>
   Right,
   <% end %>
   <% if c.left == "Yes" %>
   Left,
   <% end %>
   <% if c.top == "Yes" %>
   Top,
   <% end %>
   <% if c.bottom == "Yes" %>
   Bottom
   <% end %>
</td>
   <td><%= c.quantity %></td>

   <% @sq = ((((c.width*c.depth)/92903)*2)*c.quantity)+((((c.height*c.depth)/92903)*2)*c.quantity) %>
   <% @skirting_sqft = 0 %>
   <% @v_area = 0 %>
   <% @h_area = 0 %>
   <!-- Square feet of skirting -->
   <% if c.skirting != nil %>
   <% @s_width = c.width - 36 %>
   <% @skirting_sqft = ((@s_width*c.skirting)/92903)*c.quantity %>
   <% end %>
   <!-- end of calculation of skirting -->
   <% if c.vertical != nil %>
   <% if c.skirting != nil %>
    <% @v_height = c.height-c.skirting-36 %>
    <% else %>
    <% @v_height = c.height-36 %>
     <% end %>
     <% @v_area = ((@v_height * c.depth)/92903)*c.quantity %>

   <% end %>

   <% if c.horizontal != nil %>
   <% @h_width = c.width - 36 %>
   <% @h_area =  ((@h_width*c.depth)/92903)*c.quantity %>
   <% end %>
    
<% @sqft = @sq + @skirting_sqft  + @v_area + @h_area %>
<% @total_sqft += @sqft %>
<td><%= @sqft.round(4) %></td>
  <% @sqft_back = ((c.width*c.height)/92903)*c.quantity %>

  <% @total_sqft_back += @sqft_back %>
<td><%= @sqft_back.round(4) %></td>
   
   
   
 </tr>




  
  <% end %>
  <tr>
  <th colspan="8">Total:</th>
  <td><b><%= @total_no %></b></td>
  <td><b><%= @total_sqft.round(4) %></b></td>
  
  
  <td><b><%= @total_sqft_back.round(4) %></b></td>
  
  </tr>
  <tr>
  <th colspan="10">Total Quantity(SQ.FEET):</th>
  <td><b><%= (@total_sqft + @total_sqft_back).round(4) %></b></td>
  
  </tr>
  
  <% end %>
  <% if u == "Drawer" %>
   <tr>
    <th>No</th>
    <th>W</th>
    <th>D</th>
    <th>H</th>
    <th>Nos</th>
    <th>Sq.ft</th>
    
  </tr>
  <% @drawer_measurements = f.measurements.where(ftype: u) %>
    <% @total_no = 0 %>
    <% @total_sqft = 0 %>
    
    <% @total_amount = 0 %>
    <%# @drawer_rate = @rates.find_by(["product = ? and ptype=?",f.product,u]) %>
    <%# @d = @drawer_rate.price %>
    <% @drawer_measurements.each do |c| %>
   <% @i += 1 %>
   <% @total_no += c.quantity %>
   <tr>
   <td><%= @i %></td>
   <td><%= c.width %></td>
   <td><%= c.depth %></td>
   <td><%= c.height %></td>
   <td><%= c.quantity %></td>
   <% @sqft = ((((c.height*c.depth)/92903)*2)*c.quantity)+((((c.height*c.width)/92903)*2)*c.quantity)+(((c.width*c.depth)/92903)*c.quantity) %>
   <% @total_sqft += @sqft %>
   <td><%= @sqft.round(4) %></td>
   
  
   
   
 </tr>
 <% end %>
  <tr>
  <th colspan="4">Total:</th>
  <td><b><%= @total_no %></b></td>
  <td><b><%= @total_sqft.round(4) %></b></td>
  
  
  
  </tr>
  <tr>
  <th colspan="5">Total Quantity(SQ.FEET):</th>
  <td><b><%= @total_sqft.round(4) %></b></td>
  
  </tr>
  
  <% end %>

   <% if u == "Shutters" || u == "White BSL Shelf"  %>
   <% if u == "Shutters" %>
   <tr>
    <th colspan="2">No</th>
    <th colspan="2">W</th>
    <th colspan="2">H</th>
    <th>Nos</th>
    <th>Sq.ft</th>
    <th>Glass Shutter</th>
    <th>Handle</th>
    <th>Handle Groove</th>
    <th>Handle Fitting</th>
    
   </tr>
  <% else %>
   <tr>
    <th colspan="2">No</th>
    <th colspan="2">W/H</th>
    <th colspan="2">D</th>
    <th>Nos</th>
    <th>Sq.ft</th>
    
    
  </tr>
   <% end %>
  <!-- Calculations of rates -->
  <%# if u == "Shutters" %>
  <%# @shutter_rate = @rates.find_by(["product = ? and ptype=? ",f.product,u])%>
  <%# @sr = @shutter_rate.price %>
  <%# elsif u == "White BSL Shelf" %>
  <%# @wbs_rate = @rates.find_by(["product = ? and ptype=? ",f.product,u])%>
  <%# @sr = @wbs_rate.price %>
  <%# elsif u == "Hinges Patti" %>
  <%# @hp_rate = @rates.find_by(["product = ? and ptype=? ",f.product,u])%>
  <%# @sr = @hp_rate.price %>
  <%# end %>
  <!-- End of calculation -->
  <% @g_area = 0 %>
  <% @h_rate = 0 %>
  <% @hg_rate = 0 %>
  <% @hf_rate = 0 %>

  <% @shutter_measurements = f.measurements.where(ftype: u ) %>
  <% @tot_sqrft = 0 %>
  <% @tot_num = 0 %>
  <% @full_amt = 0 %>

   <% @shutter_measurements.each do |c| %>
   <% if u == "Shutters" %>
   <% if c.glass_shutter == "Yes" %>
   <% @g_area = ((c.width * c.height)/92903)*c.quantity %>
   <% end %> 
   <% if c.handle == "Yes" %>
   <% @h_rate = ((c.width/305)*c.quantity)*20 %>
   
   <% end %> 
   <% if c.handle_groove == "Yes" %>
   <% @hg_rate = c.quantity * 20 %>
   <% end %> 
   <% if c.handle_fitting == "Yes" %>
   <% @hf_rate = c.quantity * 20 %>
   <% end %> 
   <% end %>
  
   <% @i += 1 %>
   <tr>
   <td colspan="2"><%= @i %></td>
   <% if u == "White BSL Shelf" %>
   <td colspan="2"><%= c.wh %></td>
   <td colspan="2"><%= c.depth %></td>
   <% else %>
   <td colspan="2"><%= c.width %></td>
   <td colspan="2"><%= c.height %></td>
   <% end %>
   
   <td><%= c.quantity %></td>
   <% if u == "White BSL Shelf" %>
   <% @sqrft = ((c.wh.to_f*c.depth)/92903)*c.quantity %>
   <% else %>
   <% @sqrft = ((c.width*c.height)/92903)*c.quantity %>
   <% end %>
   <td><%= @sqrft.round(4) %></td>
   <% @tot_sqrft += @sqrft %>
   <% if u == "Shutters" %>
   <td><%= c.glass_shutter %></td>
   <td><%= c.handle %></td>
   <td><%= c.handle_groove %></td>
   <td><%= c.handle_fitting %></td>
   <% end %>

   
  
   
   
  

  </tr>
  <% @tot_num += c.quantity %>
  <% end %>
  <tr>
  <th colspan="6">Total:</th>

  <td><b><%= @tot_num %></b> </td>
  <td><b><%= @tot_sqrft.round(4) %></b></td>
  
  

  </tr>
  <tr>
  <th colspan="7">Total Quantity(SQ.FEET):</th>
  <td><b><%= @tot_sqrft.round(4) %></b></td>
 

  </tr>
  
  <% end %>
  <% end %>
 
  
 
  
  
  

  


 
  



   
</table>
<% end %>
